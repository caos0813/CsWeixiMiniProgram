/**app.wxss**/
.line {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 1rpx;
  background-color: #e5e5e5;
  z-index: 999;
}
scroll-view {
  box-sizing: border-box;
}
page {
  color: #333;
}
.clearfix::after,.clearfix::after{
  display: table;
  content: " ";
}
.clearfix::after{
  clear: both;
}
